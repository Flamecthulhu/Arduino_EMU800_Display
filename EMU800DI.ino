  {
    for(;;);
  }
  display.clearDisplay();
  display.drawBitmap(x, y, welcomescreen, screen_width, sceeen_height, 1);
  display.display();
  delay(welcome_screen_time);
  CleanAllScreen();
  pinMode(6, OUTPUT);
  digitalWrite(6, LOW);
}

